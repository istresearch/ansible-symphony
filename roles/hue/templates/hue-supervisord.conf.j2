[program:hue]
command={{ hue_install_dir }}/build/env/bin/hue runserver
autostart=true
autorestart=true
startsecs={{ hue_start_secs }}
startretries={{ hue_start_retries }}
stopsignal=SIGTERM
stopwaitsecs=600
redirect_stderr=true
stdout_logfile=AUTO
stdout_logfile_maxbytes={{ hue_stdout_logfile_maxbytes }}
stderr_logfile=AUTO
stderr_logfile_maxbytes={{ hue_stderr_logfile_maxbytes }}
environment = {{ hue_env_vars }}
