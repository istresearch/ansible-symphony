[program:tangelo]
command=/usr/local/bin/tangelo -c {{ tangelo_conf_dir }}/settings.yml -r {{ tangelo_webroot }}
autostart=true
autorestart=true
startsecs={{ tangelo_start_secs }}
startretries={{ tangelo_start_retries }}
stopsignal=KILL
redirect_stderr=true
stdout_logfile=AUTO
stdout_logfile_maxbytes={{ tangelo_stdout_logfile_maxbytes }}
stderr_logfile=AUTO
stderr_logfile_maxbytes={{ tangelo_stderr_logfile_maxbytes }}