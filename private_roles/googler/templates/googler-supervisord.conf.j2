[program:googler]
command=/usr/bin/python googler.py
directory=/opt/googler/default/googler
autostart=true
autorestart=true
startsecs=5
startretries=100
stopwaitsecs=10
redirect_stderr=true
stdout_logfile=AUTO
stdout_logfile_maxbytes=50MB
stderr_logfile=AUTO
stderr_logfile_maxbytes=50MB
environment = DISPLAY=:{{ xvfb_display }}