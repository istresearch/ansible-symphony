## PUT VARIABLES THAT PERTAIN TO BOTH MASTER AND DATA NODES IN THIS FILE

datadog_monitors:
  supervisord:
    enabled: true
    is_custom_check: false
    server_name: '{{ inventory_hostname }}'
  elastic:
    enabled: true
    is_custom_check: false
    es_instances:
        - url: http://localhost:9200
          tag: "es-cluster:ist"

